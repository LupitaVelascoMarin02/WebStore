<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="WebStore" dynamic-insert="false" dynamic-update="false" mutable="true" name="classes.Billing" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="billing">
        <id name="idBilling" type="java.lang.Integer">
            <column name="IdBilling"/>
            <generator class="identity"/>
        </id>
        <property generated="never" lazy="false" name="invoice" optimistic-lock="true" type="string" unique="false">
            <column length="50" name="Invoice" unique="true"/>
        </property>
        <property generated="never" lazy="false" name="amount" optimistic-lock="true" type="java.lang.Double" unique="false">
            <column name="Amount" precision="22" scale="0"/>
        </property>
        <property generated="never" lazy="false" name="date" optimistic-lock="true" type="date" unique="false">
            <column length="10" name="Date" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="idPOrder" optimistic-lock="true" type="int" unique="false">
            <column name="IdP_order" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
