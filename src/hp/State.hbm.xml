<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="WebStore" dynamic-insert="false" dynamic-update="false" mutable="true" name="classes.State" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="state">
        <id name="idState" type="java.lang.Integer">
            <column name="IdState"/>
            <generator class="identity"/>
        </id>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="string" unique="false">
            <column length="50" name="Name" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="idCountry" optimistic-lock="true" type="int" unique="false">
            <column name="IdCountry" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>
