<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="true" default-access="property" default-cascade="none" default-lazy="true">
    <class catalog="WebStore" dynamic-insert="false" dynamic-update="false" mutable="true" name="classes.Client" optimistic-lock="none" polymorphism="implicit" select-before-update="false" table="client">
        <id name="idClient" type="java.lang.Integer">
            <column name="IdClient"/>
            <generator class="identity"/>
        </id>
        <property generated="never" lazy="false" name="name" optimistic-lock="true" type="string" unique="false">
            <column length="25" name="Name" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="lastName" optimistic-lock="true" type="string" unique="false">
            <column length="50" name="LastName" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="username" optimistic-lock="true" type="string" unique="false">
            <column length="50" name="Username" not-null="true" unique="true"/>
        </property>
        <property generated="never" lazy="false" name="password" optimistic-lock="true" type="string" unique="false">
            <column length="50" name="Password" not-null="true"/>
        </property>
        <property generated="never" lazy="false" name="email" optimistic-lock="true" type="string" unique="false">
            <column length="25" name="Email" not-null="true" unique="true"/>
        </property>
    </class>
</hibernate-mapping>
